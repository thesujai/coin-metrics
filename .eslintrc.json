{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "on",
    "consistent-return": "off",
    "arrow-body-style": ["error", "as-needed"],
    "no-underscore-dangle": "off"
  }
}
